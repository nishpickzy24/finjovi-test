{
  "rewrites": [
    {
      "source": "/:path",
      "destination": "/:path.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path.html",
      "destination": "/:path",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ]
}
